
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <link href="~/Content/css/login.css" rel="stylesheet" />
</head>
<body>
    <div>

            <div class="container-i">
                <div class="card">
                    <div class="card-body">
                        <div class="card-header">
                            <p>Inicia Sesión</p>
                        </div>
                        <div class="card-body">
                            <label c>Correo electrónico</label>
                            <input class="form-control-range" placeholder="Ingresar correo" id="login-email">

                            <label c class="mt-3">Contraseña</label>
                            <input class="form-control-range icon-o" placeholder="Ingresar Contraseña" id="login-contrasenia">
                            <label c class="mt-3">Token</label>
                            <input class="form-control-range icon-o" placeholder="Ingresar Token" id="login-token">
                            <a href='@Url.Action("Index", "CrearCuenta")'>Crear Cuenta</a>

                            <button class="btn btn-primary form-control mt-5" btn-i id="ingreso_credenciales">Ingresar</button>
                            <div class="card-info text-muted pt-3">
                                <button class="btn form-control" btn-c>Continuar con Google</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

    </div>
</body>
</html>


<script src="~/Content/js/login.js"></script>

